{
  "name": "angular-morris-js",
  "version": "0.2.2",
  "keywords": [
    "angular",
    "angular 4",
    "angular 5",
    "typescript",
    "admin-lte"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng-packagr -p ng-packagr.json"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mika-el/angular-morris-js.git"
  },
  "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
  "ngPackage": {
    "src": "src",
    "dest": "dist",
    "workingDirectory": ".ng_build",
    "lib": {
      "entryFile": "index.ts"
    }
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "admin-lte": "^2.4.2",
    "angular-admin-lte": "^0.2.7",
    "angular-loading-page": "^0.4.2",
    "bootstrap": "^3.3.7",
    "codelyzer": "4.3.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "^3.3.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "morris.js": "^0.5.0",
    "ng-packagr": "^1.7.0",
    "prismjs": "^1.11.0",
    "protractor": "~5.1.2",
    "raphael": "^2.2.7",
    "ts-node": "~4.1.0",
    "tslint": "5.10.0",
    "typescript": "2.7.2",
    "@angular-devkit/build-angular": "~0.6.0"
  }
}
